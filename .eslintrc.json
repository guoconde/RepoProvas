{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off",
        "react/jsx-filename-extension": "off",
        "no-console": "off",
        "react/jsx-props-no-spreading": "off"
    }
}
